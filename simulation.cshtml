@using System;
@using System.Diagnostics;
@using System.ComponentModel;
@{
    string add = "";
    string[] fout = new string[10];
    if(IsPost){
        System.Diagnostics.Process process = new System.Diagnostics.Process();
        System.Diagnostics.ProcessStartInfo startInfo = new System.Diagnostics.ProcessStartInfo();
        startInfo.WindowStyle = System.Diagnostics.ProcessWindowStyle.Hidden;
        add = "2" + " " + Request["vl"] + " " + Request["nv"] + " " + Request["its"] + " " + Request["dcw"];
        startInfo.FileName = "cmd.exe";
        startInfo.Arguments = "/C g++ C:/Programming/624/Project/CodeFiles/run.cpp -o C:/Programming/624/Project/run -std=c++11 && C:/Programming/624/Project/run.exe " + add;
        process.StartInfo = startInfo;
        process.Start();
        process.WaitForExit();
        <text>@process.ExitCode</text>
        System.IO.StreamReader file = new System.IO.StreamReader("c:/Programming/624/Project/CodeFiles/newout.txt");
        string line;
        for(int i = 0; i < 5; i++){
            fout[i+5] = file.ReadLine();
            fout[i] = file.ReadLine();
        }
        file.Close();
    }
}
